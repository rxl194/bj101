<!DOCTYPE html>
<html xmlns:ng="http://angularjs.org">
<head>
  <!-- Use the 'title' property to render a page title  -->
  <title><%= title %></title>
  <link href="/coa/todolists/css/bootstrap.css" rel="stylesheet" />
  <link href="/coa/todolists/css/bootstrap-theme.css" rel="stylesheet" />
</head>
<body>
  <!-- Render the user object -->
  <script type="text/javascript">
    window.admin_user = <%- admin_user || 'null' %>;
  </script>

  <!-- Render AngularJS views  -->
  <section ng-view></section>


  <!-- Load local libraries -->
  <script type="text/javascript" src="/lib/angular/angular.js"></script>
  <script type="text/javascript" src="/lib/angular-route/angular-route.js"></script>
  <script type="text/javascript" src="/lib/angular-resource/angular-resource.js"></script>

  <!-- Load the users module -->
  <script type="text/javascript" src="/cys/users/cys_users.module.js"></script>
  <script type="text/javascript" src="/cys/users/services/authentication.client.service.js"></script>

  <!-- Load the csps_cart moduel -->
  <script type="text/javascript" src="/csps/cart/csps_cart.module.js"></script>
  <script type="text/javascript" src="/csps/cart/services/csps_orders.service.js"></script>

  <!-- Load the sps_admin module -->
  <script type="text/javascript" src="/csps/admin/csps_admin.module.js"></script>
  <script type="text/javascript" src="/csps/admin/controllers/csps_admin.controller.js"></script>
  <script type="text/javascript" src="/csps/admin/controllers/csps_admin.main.controller.js"></script>
  <script type="text/javascript" src="/csps/admin/controllers/csps_admin.orders.controller.js"></script>
  <script type="text/javascript" src="/csps/admin/config/csps_admin.routes.js"></script>

  <!-- Bootstrap AngularJS application -->
  <script type="text/javascript" src="/cex/proangular/cex_proangular_sps_admin.js"></script>


</body>
</html>

