<!DOCTYPE html>
<html xmlns:ng="http://angularjs.org">
<head>
  <!-- Use the 'title' property to render a page title  -->
  <title><%= title %></title>
  <link href="/coa/todolists/css/bootstrap.css" rel="stylesheet" />
  <link href="/coa/todolists/css/bootstrap-theme.css" rel="stylesheet" />
</head>
<body>
  <!-- Render the user object -->
  <script type="text/javascript">
    window.user = <%- cys_user || 'null' %>;
  </script>

  <!-- Render AngularJS views  -->
  <section data-ng-controller="cspsStoreController" data-ng-init="find()">
    <div class="navbar navbar-inverse">
      <a class="navbar-brand" href="#!">SPORTS STORE</a>
      <csps-cart-summary />
    </div>

    <div class="alert alert-danger" ng-show="data.error">
      Error ({{data.error.status}}). The product data was not loaded. 
      <a ng-href="{{pathExProangularSps}}" class="alert-link">Click here to try again</a>
    </div>

    <ng-view />

  </section>


  <!-- Load local libraries -->
  <script type="text/javascript" src="/lib/angular/angular.js"></script>
  <script type="text/javascript" src="/lib/angular-route/angular-route.js"></script>
  <script type="text/javascript" src="/lib/angular-resource/angular-resource.js"></script>

  <!-- Load the users module -->
  <script type="text/javascript" src="/cys/users/cys_users.module.js"></script>
  <script type="text/javascript" src="/cys/users/services/authentication.client.service.js"></script>

  <!-- Load the csps_products module -->
  <script type="text/javascript" src="/csps/products/csps_products.module.js"></script>
  <script type="text/javascript" src="/csps/products/services/csps_products.service.js"></script>
  <script type="text/javascript" src="/csps/products/controllers/csps_catprod.controller.js"></script>

  <!-- Load the csps_cart moduel -->
  <script type="text/javascript" src="/csps/cart/csps_cart.module.js"></script>
  <script type="text/javascript" src="/csps/cart/services/csps_cart.service.js"></script>
  <script type="text/javascript" src="/csps/cart/services/csps_orders.service.js"></script>
  <script type="text/javascript" src="/csps/cart/directive/csps_cart.directive.js"></script>
  <script type="text/javascript" src="/csps/cart/controllers/csps_cartsum.controller.js"></script>

  <!-- Load the coa-todolists module -->
  <script type="text/javascript" src="/csps/store/csps_store.module.js"></script>
  <script type="text/javascript" src="/csps/store/controllers/csps_store.controller.js"></script>
  <script type="text/javascript" src="/csps/store/config/csps_store.routes.js"></script>
  <!--
  <script type="text/javascript" src="/coa/todolists/services/coa_todolists.service.js"></script>
  <script type="text/javascript" src="/coa/todolists/filters/coa_todolists.filter.js"></script>
  -->

  <!-- cutil_filters module -->
  <script type="text/javascript" src="/cuti/filters/cutil_uniqueProperty.filters.js"></script>

  <!-- Bootstrap AngularJS application -->
  <script type="text/javascript" src="/cex/proangular/cex_proangular_sportsstore.js"></script>


</body>
</html>

